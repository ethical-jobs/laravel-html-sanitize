{
    "name": "ethical-jobs/laravel-html-sanitize",
    "type": "library",
    "description": "Laravel html sanitization",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ezyang/htmlpurifier": "^4.14"

    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^6.25",
        "orchestra/database": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "EthicalJobs\\Sanitize\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "EthicalJobs\\Sanitize\\ServiceProvider"
            ]
        }
    }
}
